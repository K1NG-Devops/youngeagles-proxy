{
  "version": 2,
  "rewrites": [
    {
      "source": "/(login|register|projects|admin|parent|child|payment|confirmed)(.*)",
      "destination": "https://angular-app-eight-teal.vercel.app/$2"
    },
    {
      "source": "/home",
      "destination": "https://react-app-rho-ruddy.vercel.app/"
    },
    {
      "source": "/home/:path*",
      "destination": "https://react-app-rho-ruddy.vercel.app/:path*"
    },
    {
      "source": "/about",
      "destination": "https://react-app-rho-ruddy.vercel.app/about"
    },
    {
      "source": "/about/:path*",
      "destination": "https://react-app-rho-ruddy.vercel.app/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "https://angular-app-eight-teal.vercel.app/"
    }
  ]
}

